<div class="cta-section-press">
  <div class="container-large">
    <div class="cta-content-wrapper">
      <h3 class="cta-heading-category cta-heading-press">Join Our Email List</h3>
      <div class="bottom-form-block-brand w-form">

        {% form 'customer' %}
        {% if form.posted_successfully? %}
          <div class="jc-form-done">
            <div>Thank you! Your subscription has been confirmed!</div>
          </div>
        {% else %}
          <input type="hidden" name="contact[accepts_marketing]" value="true">
          <input type="email" class="bottom-form-input-brand bottom-form-input-press w-input" maxlength="256" name="contact[email]" data-name="Email 4" id="email" required="">
          <input type="submit" value="Sign Up" data-wait="Please wait..." class="btn-main w-button">
          {% endif %}
        {% endform %}

        <div class="w-form-done">
          <div>Thank you! Your submission has been received!</div>
        </div>
        <div class="w-form-fail">
          <div>Oops! Something went wrong while submitting the form.
          </div>
        </div>

      </div>

    </div>
  </div>
</div>

